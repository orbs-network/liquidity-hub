pragma solidity 0.8.x;

import "forge-std/Test.sol";

import {DeployPermit2} from "permit2/test/utils/DeployPermit2.sol";
import {PermitSignature} from "uniswapx/test/util/PermitSignature.sol";

contract DeployInfra is DeployPermit2, PermitSignature {
    address public constant MULTICALL3 = 0xcA11bde05977b3631167028862bE2a173976CA11;

    function deployInfra() public returns (address) {
        vm.chainId(137); // needed for permit2

        deployPermit2();
        require(PERMIT2_ADDRESS != address(0), "DeployPermit2 failed");

        _deployMulticall3();

        bytes memory bytecode =
            hex"";

        address reactor = makeAddr("ExclusiveDutchOrderReactor");
        vm.etch(reactor, bytecode);
        return reactor;
    }

    function _deployMulticall3() private {
        bytes memory bytecode =
            hex"";

        vm.etch(MULTICALL3, bytecode);
    }
}
